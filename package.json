{
	"name": "dav-ui-components",
	"version": "1.0.3",
	"description": "A collection of UI components used in our web projects",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsc; rm -rf docs; pitsby build",
		"dev": "rm -rf docs; concurrently npm:compile:watch npm:docs:watch",
		"prepare": "npm run build",
		"compile:watch": "tsc -w",
		"docs:watch": "pitsby build --watch"
	},
	"license": "MIT",
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.4.2",
		"autosize": "^6.0.1",
		"blurhash": "^2.0.5",
		"lit": "^2.8.0"
	},
	"devDependencies": {
		"@glorious/pitsby": "^1.36.1",
		"concurrently": "^8.2.1",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2"
	}
}
